{
  "name": "foody-order",
  "version": "0.0.0",
  "scripts": {
    "start": "vite --port 4173",
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --port 4173 --host",
    "test": "vitest run --environment jsdom",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "vitest run && start-server-and-test start http://localhost:4173/ 'cypress run --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.8.3",
    "aos": "^3.0.0-beta.6",
    "axios": "^1.6.2",
    "axios-cache-interceptor": "^1.3.2",
    "pinia": "^2.1.7",
    "start-server-and-test": "^2.0.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vue-tel-input": "^8.1.4",
    "vue-toast-notification": "^3.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.5.1",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.2",
    "autoprefixer": "^10.4.16",
    "c8": "^8.0.1",
    "cross-fetch": "^4.0.0",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
