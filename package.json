{
  "name": "foody-order",
  "version": "0.0.0",
  "scripts": {
    "start": "vite --port 4173",
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --port 4173 --host",
    "test": "vitest run --environment jsdom",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "vitest run && start-server-and-test start http://localhost:4173/ 'cypress run --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.29.20",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0",
    "pinia": "^2.0.35",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-tel-input": "^8.1.1",
    "vue-toast-notification": "^3.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@pinia/testing": "^0.0.16",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.8",
    "c8": "^7.13.0",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.17.0",
    "eslint": "^8.21.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "vite": "^3.0.4",
    "vitest": "^0.21.0"
  },
  "overrides": {
    "semver": "~7.5.2"
  }
}
